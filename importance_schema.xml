<?xml version="1.0"?>
<schema xmlns="http://www.w3.org/2000/10/XMLSchema">
  <!-- XSchema definition for Personal Associations -->
  <element name="Relationship">
    <complexType>
	  <attribute name="1. Person 1">
        <simpleType>
          <restriction base="string">
			<enumeration value="ID Number and Text Span"/>
			<enumeration value="Current Speaker"/>
          </restriction>
		</simpleType>   
       </attribute>
      <attribute name="3. Type of Relationship">
        <simpleType>
          <restriction base="string">
			<enumeration value="Mother Of"/>
			<enumeration value="Father Of"/>
			<enumeration value="Friend Of"/>
			<enumeration value="Neighbor Of"/>
			<enumeration value="Son Of"/>
			<enumeration value="Daughter Of"/>
			<enumeration value="Granddaughter Of"/>
			<enumeration value="Grandson Of"/>
			<enumeration value="Brother Of"/>
			<enumeration value="Sister Of"/>
			<enumeration value="Cousin Of"/>
			<enumeration value="Ex-Wife Of"/>
			<enumeration value="Ex-Husband Of"/>
			<enumeration value="Co-Worker Of"/>
			<enumeration value="Boss Of"/>
			<enumeration value="Doctor Of"/>
			<enumeration value="Nurse Of"/>
			<enumeration value="Therapist Of"/>
			<enumeration value="Niece Of"/>
			<enumeration value="Nephew Of"/>
			<enumeration value="Physical Therapist Of"/>
			<enumeration value="Acquaintance Of"/>
			<enumeration value="Siblings Of"/>
			<enumeration value="Wife Of"/>
			<enumeration value="Husband Of"/>
			<enumeration value="Grandmother Of"/>
			<enumeration value="Grandfather Of"/>
			<enumeration value="Aunt Of"/>
			<enumeration value="Uncle Of"/>
			<enumeration value="Step-mother Of"/>
			<enumeration value="Step-father Of"/>
			<enumeration value="Step-brother Of"/>
			<enumeration value="Step-sister Of"/>
			<enumeration value="Step-siblings Of"/>
			<enumeration value="Step-son Of"/>
			<enumeration value="Step-daughter Of"/>
			<enumeration value="Granddaughter-in-law Of"/>
			<enumeration value="Grandson-in-law Of"/>
			<enumeration value="Daughter-in-law Of"/>
			<enumeration value="Son-in-law Of"/>
			<enumeration value="Father-in-law Of"/>
			<enumeration value="Mother-in-law Of"/>
			<enumeration value="Sister-in-law"/>
			<enumeration value="Brother-in-law Of"/>
			<enumeration value="Trainer Of"/>
			<enumeration value="Boyfriend Of"/>
			<enumeration value="Girlfriend Of"/>
			<enumeration value="Romantic Partner Of"/>
			<enumeration value="Partner Of"/>
			<enumeration value="Children Of"/>
			<enumeration value="Unknown"/>
			<enumeration value="Family Of"/>
			<enumeration value="Grandchildren Of"/>
		  </restriction>
        </simpleType>   
       </attribute>
	   <attribute name="4. Importance of Relationship according to Person 1">
        <simpleType>
          <restriction base="string">
			<enumeration value="1--Neutral"/>
			<enumeration value="2--Sort of Important"/>
			<enumeration value="3--Important"/>
			<enumeration value="4--Very Important"/>
          </restriction>
		</simpleType>   
       </attribute>
	   <attribute name="5. Certainty of Importance of Relationship according to Person 1">
        <simpleType>
          <restriction base="string">
			<enumeration value="0"/>
			<enumeration value="1"/>
			<enumeration value="2"/>
			<enumeration value="3"/>
			<enumeration value="4"/>
			<enumeration value="5"/>
			<enumeration value="6"/>
			<enumeration value="7"/>
			<enumeration value="8"/>
			<enumeration value="9"/>
			<enumeration value="10"/>
          </restriction>
		</simpleType>   
       </attribute>
	    <attribute name="7. Importance of Relationship according to Person 2">
        <simpleType>
          <restriction base="string">
			<enumeration value="1--Neutral"/>
			<enumeration value="2--Sort of Important"/>
			<enumeration value="3--Important"/>
			<enumeration value="4--Very Important"/>
          </restriction>
		</simpleType>   
       </attribute>
	   <attribute name="8. Certainty of Importance of Relationship according to Person 2">
        <simpleType>
          <restriction base="string">
			<enumeration value="0"/>
			<enumeration value="1"/>
			<enumeration value="2"/>
			<enumeration value="3"/>
			<enumeration value="4"/>
			<enumeration value="5"/>
			<enumeration value="6"/>
			<enumeration value="7"/>
			<enumeration value="8"/>
			<enumeration value="9"/>
			<enumeration value="10"/>
          </restriction>
		</simpleType>   
       </attribute>
	   <attribute name="6. Person 2">
        <simpleType>
          <restriction base="string">
		  <enumeration value="ID Number and Text Span"/>
			<enumeration value="Current Speaker"/>
          </restriction>
		</simpleType>   
       </attribute>
	   <attribute name="9. Quality of Relationship">
        <simpleType>
          <restriction base="string">
			<enumeration value="1--Very Bad"/>
			<enumeration value="2--Bad"/>
			<enumeration value="3--Neutral"/>
			<enumeration value="4--Good"/>
			<enumeration value="5--Very Good"/>
          </restriction>
		</simpleType>   
       </attribute>
	   <attribute name="10. Percent Certainty of Quality of Relationship">
        <simpleType>
          <restriction base="string">
			<enumeration value="0"/>
			<enumeration value="1"/>
			<enumeration value="2"/>
			<enumeration value="3"/>
			<enumeration value="4"/>
			<enumeration value="5"/>
			<enumeration value="6"/>
			<enumeration value="7"/>
			<enumeration value="8"/>
			<enumeration value="9"/>
			<enumeration value="10"/>
          </restriction>
		</simpleType>   
       </attribute>
    </complexType>
  </element>
</schema>

